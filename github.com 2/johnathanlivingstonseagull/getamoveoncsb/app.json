{
    "expo": {
      "name": "Zoorted",
      "slug": "zoorted",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/roughlogo.jpg",
      "splash": {
        "image": "./assets/bicyle-man.png", 
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "updates": {
        "fallbackToCacheTimeout": 0
