    "android": {
      "adaptiveIcon": {
        "foregroundImage": "/workspaces/GetaMoveOnCSB/src/assets/bicycle-man.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "plugins": [
      "expo-location",
      "expo-image-picker",
      "expo-notifications"],
      
    "web": {
      "favicon": "/workspaces/GetaMoveOnCSB/src/assets/bicycle-man.png"
    }
  }
