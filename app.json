{
  "expo": {
    "name": "Zoorted",
    "slug": "zoorted",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "/workspaces/GetaMoveOnCSB/src/assets/roughlogo.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "/workspaces/GetaMoveOnCSB/src/assets/bicycle-man.png",
